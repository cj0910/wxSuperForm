.page {
  height: 100%;
}

.container {
  padding: 0px;
  padding-top: 10px;
  background-color: #fff;
}

.forms-item {
  display: flex;
  flex-direction: column;
  padding: 5px;
  margin: 10px 5px;
  border-radius: 5px;
  background-color: #f8f8f8;
}

.form-Title {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  border-bottom: 1px solid rgba(253, 251, 251, 0);
}

.form-title {
  font-size: 30rpx;
  font-weight: 500;
  padding-top: 5px;
  padding-bottom: 7px;
}

.form-Info {
  display: flex;
  flex-direction: row;
  padding: 2px 2px;
}

.form-Info-right {
  display: flex;
  flex-direction: column;
  float: right;
  margin-left: auto;
  margin-right: 6px;
  text-align: center;
  font-size: 18rpx;
}

.form-answer {
  font-size: 18rpx;
  margin-right: 7px;
  margin-left: 2px;
  padding: 2px;
}

.date {
  font-size: 18rpx;
  border-left: 8rpx solid rgba(60, 240, 24, 0.868);
  padding-left: 10rpx;
  margin-top: 30rpx;
}

.empty {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.empty .title {
  font-size: 60rpx;
  margin: 200rpx 50rpx 50rpx;
  color: #9B9B9B;
}

.empty .content {
  color: #DDD;
  text-align: center;
}